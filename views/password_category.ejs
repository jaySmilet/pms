<%- include('header') %>
  <div class="container my-5">
    <h3 class="text-center text-danger">Password Category List</h3>
      <div class="row">
          <table class="table table-striped">
              <thead>
                  <tr>
                      <th>Password Category Name</th>
                      <th>Action</th>
                  </tr>
              </thead>
              <tbody>
                <% if(records.length >0){ 
                  records.forEach(function(row){

                 
                  %>
                  <tr>
                      <td><%= row.password_category%></td>
                      <td>
                          <a href="/passwordCategory/edit/<%= row._id%>" class="btn btn-primary"><i class="fas fa-edit"></i></a>
                          <a href="/passwordCategory/delete/<%= row._id%>" class="btn btn-danger"><i class="far fa-trash-alt"></i></a>
                      </td>
                  </tr>
                  <%  }) } %>
              </tbody>
          </table>
      </div>
  </div>
  
  <%- include('footer') %>